{
  "editor.colorDecoratorsLimit": 5000,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.defaultColorDecorators": true,
  "editor.definitionLinkOpensInPeek": true,
  "editor.gotoLocation.alternativeImplementationCommand": "editor.action.revealDefinition",
  "editor.gotoLocation.alternativeReferenceCommand": "editor.action.referenceSearch.trigger",
  "editor.linkedEditing": true,
  "editor.peekWidgetDefaultFocus": "editor",
  "editor.renderLineHighlight": "all",
  "editor.scrollbar.horizontal": "visible",
  "editor.semanticHighlighting.enabled": true,
  "editor.stablePeek": true,
  "editor.tabFocusMode": true,
  "editor.tabCompletion": "on",
  "editor.semanticTokenColorCustomizations": {
    "[Snazzy]": {}
  },
  "editor.tokenColorCustomizations": {
    "functions": "#fffb00",
    "numbers": "#0011ff"
  },
  "editor.inlineSuggest.showToolbar": "always",
  "editor.quickSuggestions": {
    "other": "inline",
    "comments": "inline",
    "strings": "inline"
  },
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showStatusBar": true,
  "files.autoSave": "onFocusChange",
  "workbench.list.typeNavigationMode": "trigger",
  "workbench.colorTheme": "Hyper Term Black",
  "workbench.preferredDarkColorTheme": "Hyper Term Black",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.settings.editor": "json",
  "workbench.settings.openDefaultKeybindings": true,
  "workbench.settings.openDefaultSettings": true,
  "workbench.settings.useSplitJSON": true,
  "search.showLineNumbers": true,
  "debug.console.acceptSuggestionOnEnter": "on",
  "problems.defaultViewMode": "table",
  "remote.extensionKind": {
    ui
    "pub.name": ["ui"]
  }
}
